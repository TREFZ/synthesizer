{
  "name": "synthesizer",
  "version": "0.1.0",
  "description": "A cheap, lightweight, fast way to run health checks and synthetic tests on an interval.",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node --env-file .env index.js",
    "dev": "node --env-file .env --watch index.js",
    "test": "node --test --test-reporter=spec \"./test/nodejs/specs/**/*.js\"",
    "test:watch": "node --test --test-reporter=spec --watch \"./test/nodejs/specs/**/*.js\""
  },
  "author": "Business Insider",
  "license": "UNLICENSED",
  "devDependencies": {
    "cheerio": "^1.0.0",
    "node-test-parser": "^2.2.2"
  },
  "dependencies": {
    "alpinejs": "^3.14.1",
    "express": "^4.21.0"
  }
}
